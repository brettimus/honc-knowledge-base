{
	"name": "honc-docs-indexer",
	"type": "module",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"codegen": "tsx src/code-gen/index.ts",
		"trace": "tsx src/code-gen/utils/scripts/latest-trace.ts",
		"index:cloudflare": "tsx src/rag/scripts/index-cloudflare.ts",
		"start": "rm -rf dist && tsup && node dist/index.js",
		"dev": "tsup --watch && node dist/index.js",
		"format": "biome check . --write",
		"lint": "biome lint .",
		"agent": "tsx src/rag/agent-experiment.ts",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@ai-sdk/mistral": "^0.0.44",
		"@ai-sdk/openai": "^0.0.68",
		"@types/node": "^22.7.7",
		"@xenova/transformers": "^2.17.2",
		"ai": "^3.4.16",
		"dotenv": "^16.4.5",
		"llamaindex": "^0.7.0",
		"tsup": "^8.3.0",
		"typescript": "^5.5.4",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"chalk": "^5.3.0",
		"cli-highlight": "^2.1.11",
		"highlight.js": "^11.10.0",
		"tsx": "^4.19.1"
	}
}
